{
  "name": "klea-merkuri-portofolio",
  "private": true,
  "description": "A creative minimalist developer portfolio built with Typescript, Gatsby and Bootstrap integrated with Google Gemini.",
  "version": "1.5.0",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "start": "gatsby clean && gatsby develop",
    "serve": "gatsby serve",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "predeploy": "gatsby clean",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint '**/*.{js,jsx,ts,tsx}' --fix",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md}'"
  },
  "dependencies": {
    "@google/genai": "^0.9.0",
    "@supabase/supabase-js": "^2.49.4",
    "@xenova/transformers": "^2.17.2",
    "bootstrap": "^5.3.8",
    "gatsby": "^5.14.1",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sass": "^6.14.0",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-json": "^5.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "html-react-parser": "^5.2.2",
    "react": "^17.0.0 || ^18.0.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^17.0.0 || ^18.0.0",
    "sass": "^1.83.0",
    "uuid": "^11.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thehelpfultipper/kleamerkuri"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-webpack-plugin": "^4.2.0",
    "gh-pages": "^6.1.1",
    "globals": "^15.14.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
